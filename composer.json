{
    "name": "xterr/supervisor-bundle",
    "type": "library",
    "description": "Symfony bundle which allows you to use @Supervisor annotations to configure how Supervisor runs your console commands.",
    "keywords": ["supervisor", "supervisord", "symfony", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Razvan Ceana",
            "email": "razvan@ceana.ro"
        }
    ],
    "require": {
        "php": "^7.0",
        "francodacosta/supervisord": "^1.0",
        "symfony/console": "~4.0",
        "doctrine/annotations": "^1.6",
        "symfony/framework-bundle": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": { "Xterr\\SupervisorBundle\\": "" },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
